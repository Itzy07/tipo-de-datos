let nota= 80;
if (nota >= 90) {
    console.log("Excelente");
} else if (nota >= 75 && nota <=89) {
    console.log("Bien");
} else if (nota >= 60 && nota <=74) {
    console.log("Suficiente");
} else {
    console.log("El estudiante no aprobo");
}

let colores = ['rojo', 'verde', 'azul', 'amarillo'];
colores.shift(); 
console.log(colores);
